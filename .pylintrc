[MASTER]
disable=
    C0111, # missing-docstring
    C0103, # invalid-name
    C0301, # line-too-long
    W0511, # fixme
    R0903, # too-few-public-methods
    R0913, # too-many-arguments

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=W0311,W0312,C0330,C0326

[REPORTS]
output-format=text
reports=no

[BASIC]
good-names=i,j,k,ex,Run,_,id,db

[SIMILARITIES]
min-similarity-lines=5
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
